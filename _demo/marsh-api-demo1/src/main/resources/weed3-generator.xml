        ${tableName}
        ${domainName}
        ${entityName}
        ${fields}
        ${fields_public}
        ${fields_getter}
        ${fields_setter}
        -->
        <entityGenerator targetPackage="demo.model.db" entityName="${domainName}Do">
            <![CDATA[
import lombok.Data;

@Table("${tableName}")
public class ${entityName}{
  ${fields_public}
}
]]>
        </entityGenerator>

        <table tableName="water_cfg_properties" />
        <table tableName="water_reg_service" />
    </source>
</generator>