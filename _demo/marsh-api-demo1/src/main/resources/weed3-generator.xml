            url="jdbc:mysql://localhost:3306/water?useUnicode=true&amp;characterEncoding=utf8&amp;autoReconnect=true&amp;rewriteBatchedStatements=true"
            username="root"
            password="123456"
            driverClassName="com.mysql.cj.jdbc.Driver"
